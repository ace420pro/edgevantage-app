# Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENVIRONMENT=development

# Analytics
REACT_APP_GA_TRACKING_ID=YOUR_GA_TRACKING_ID
REACT_APP_FB_PIXEL_ID=YOUR_FB_PIXEL_ID

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_AB_TESTING=true
REACT_APP_ENABLE_EDUCATION_HUB=true
REACT_APP_ENABLE_AFFILIATE_PORTAL=true

# Error Tracking (Optional)
REACT_APP_SENTRY_DSN=

# Build Configuration
GENERATE_SOURCEMAP=false

# Example for production:
# REACT_APP_API_URL=https://api.edgevantage.com
# REACT_APP_ENVIRONMENT=production
# REACT_APP_GA_TRACKING_ID=GA-XXXXXXXXX
# REACT_APP_FB_PIXEL_ID=XXXXXXXXXXXXXXXXX